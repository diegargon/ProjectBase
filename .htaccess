RewriteEngine On
RewriteBase /

RewriteRule  ^es\/news/([0-9]+)/([0-9]+)/searchTag/(.*)$   newspage.php?lang=es&nid=$1&page=$2&searchTag=$3 [NC]
RewriteRule  ^en\/news/([0-9]+)/([0-9]+)/searchTag/(.*)$   newspage.php?lang=en&nid=$1&page=$2&searchTag=$3 [NC]

RewriteRule  ^es\/news/([0-9]+)/([0-9]+)/(.*)$   newspage.php?lang=es&nid=$1&page=$2 [NC]
RewriteRule  ^en\/news/([0-9]+)/([0-9]+)/(.*)$   newspage.php?lang=en&nid=$1&page=$2 [NC]

RewriteRule ^(es|en)(.*)$  $2?lang=$1 [QSA]

